impl for_loop => statement {
    parse {
        "for"
        $name_id: index
        "in"
        $expr: iter
        $block: code
    }

    run {
        i = 0;
        while len(iter) > i {
            $index = &iter[i];
            code();
            i = i+1;
        }
    }
}

def entry () => int {
    let x: int = 10+10*(1929+10);
    x = 10;
}
